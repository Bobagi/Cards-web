{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",  // Altere para 'es6' ou 'esnext'
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "node", // Certifica-se que o TS está resolvendo módulos corretamente
    "esModuleInterop": true     // Facilita a interoperabilidade com módulos CommonJS
  },
  "include": [
    "src/**/*.ts",
    "src/*.ts",
    "src/tilt.d.ts",
    "main.ts"  ],
  "exclude": ["node_modules"]
}
